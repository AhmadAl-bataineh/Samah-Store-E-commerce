# Development environment
# API calls use Vite proxy (see vite.config.js): /api/* -> http://localhost:8080
# Leave VITE_API_BASE_URL empty to use relative paths (proxied by Vite)
VITE_API_BASE_URL=

# Proxy target for Vite dev server (used in vite.config.js)
VITE_API_PROXY_TARGET=http://localhost:8080

